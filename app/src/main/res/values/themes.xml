<resources xmlns:tools="http://schemas.android.com/tools">
  <!-- Base application theme. -->
  <style name="Theme.Companiesys" parent="Theme.MaterialComponents.DayNight.NoActionBar">
    <!-- Primary brand color. -->
    <item name="colorPrimary">@color/french_rose</item>
    <item name="colorPrimaryVariant">@color/cardinal</item>
    <item name="colorOnPrimary">@color/white_rock</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/fountain_blue</item>
    <item name="colorSecondaryVariant">@color/hippie_blue</item>
    <item name="colorOnSecondary">@color/black</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    <!-- Screen background color -->
    <item name="android:colorBackground">@color/white_rock</item>

    <item name="android:textColorPrimary">@color/black_rock</item>
    <item name="colorControlNormal">@color/white</item>
  </style>
</resources>